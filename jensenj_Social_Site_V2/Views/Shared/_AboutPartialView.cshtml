@model jensenj_Social_Site_V2.Models.Profile

@{
    ViewBag.Title = "AboutPartialView";
}

<div id="about">
    <h3>About Me:</h3>
    <p id="bio">@Model.Description</p>
    <h4>Books:</h4>
    <ul>
        @foreach (string item in Model.BookList)
        {
            <li>@item</li>
        }

    </ul>
    <h4>Movies:</h4>
    <ul>
        @foreach (string item in Model.MovieList)
        {
            <li>@item</li>
        }
    </ul>
    <h4>Food:</h4>
    <ul>
        @foreach (string item in Model.FoodList)
        {
            <li>@item</li>
        }
    </ul>

</div>
